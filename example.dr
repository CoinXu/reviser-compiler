enum Color {
  Red,
  Pink = 100,
  Blue = 200
}

struct Foo {
  enum Type {
    foo,
    bar,
    baz = 10
  }

  struct Bar {
    enum BarType {
      Foo
    }

    bool bar = true;
    optional int32 age = 1;
  }

  required string foo = "foo";
  bool baz = true;
  Color bar = Color.Red;
  Type type = Type.foo;
  int32 age = 0;
}
